<body class="fix-header fix-sidebar card-no-border">
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" /> </svg>
    </div>
    <div id="main-wrapper">
        <% include header %>
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-block">
                                <h4>Theo dõi kinh doanh > Lịch sử nạp tiền</h4>
                                <form action="buycashhistory" method="POST" role="form">
                                                <div class="form-group">
                                                    <label for="form-tittle">Từ ngày</label>
                                                    <input type="text" class="form-control" id="form-tittle" name="datestart" required autofocus size="30%">
                                                    <label for="form-tittle">Đến ngày</label>
                                                    <input type="text" class="form-control" id="form-tittle" name="dateend" required>
                                                </div>
                                                <br>
                                               <button type="submit" class="btn btn-primary">Tìm kiếm</button>
                                </form>
                                <br>
                                <div class="table-wrapper-2">
                                    <table class="table table-hover">
                                        <thead>
                                            <tr class="bg-primary">
                                                <th>Thời gian giao dịch</th>
                                                <th>Số điện thoại khách hàng</th>
                                                <th>Tiền nạp phiếu</th>
                                                <th>Tiền trả công ty</th>
                                            </tr>
                                        <tbody>
                                        </thead>
                                        <% data.rows.forEach(function(item){ %>
                                            <tr>
                                                <td><%= moment(item.created_at).format('YYYY-DD-MM h:mm:ss') %></td>
                                                <td><%= item.phone_number %></td>
                                                <td><%= item.total %></td>
                                                <% if(item.collect_flow == 0){%>
                                                <td>- <%= item.cash_value %></td>
                                                <%} else {%>
                                                <td>+ <%= item.cash_value %></td>
                                                <% } %>
                                            </tr>
                                        <% }) %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
            <% include footer %>
        </div>
    </div>
